{
    "name": "sensei/partner-sdk",
    "description": "Official PHP SDK for Sensei Partner API - Complete gamification and user engagement platform",
    "keywords": ["sensei", "partner", "api", "sdk", "gamification", "engagement", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sensei Team",
            "email": "developers@sensei.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sensei\\PartnerSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sensei\\PartnerSDK\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sensei\\PartnerSDK\\Laravel\\SenseiPartnerServiceProvider"
            ],
            "aliases": {
                "SenseiPartner": "Sensei\\PartnerSDK\\Laravel\\Facades\\SenseiPartner"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "lint": "vendor/bin/pint",
        "lint-fix": "vendor/bin/pint --preset laravel",
        "analyse": "vendor/bin/phpstan analyse src --level=8"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
